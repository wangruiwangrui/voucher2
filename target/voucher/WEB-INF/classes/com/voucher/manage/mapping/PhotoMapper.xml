<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.voucher.manage.mapper.PhotoMapper">
	<cache eviction="LRU" flushInterval="60000" size="512" readOnly="true" />
	<resultMap id="BaseResultMap" type="com.voucher.manage.model.Photo">
	    <result column="campus_id" property="campusId" jdbcType="INTEGER" />
	    <result column="open_id" property="openId" jdbcType="VARCHAR" />
	    <result column="image_url" property="imageUrl" jdbcType="VARCHAR" />
	    <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
	    <result column="nickname" property="nickname" jdbcType="VARCHAR" />
		<result column="headimg_url" property="headImgUrl" jdbcType="VARCHAR" />
	</resultMap>
	
	 <select id="getAllPhoto" resultMap="BaseResultMap" useCache="false">
	  select photo.open_id,image_url,
	    nickname,headimg_url,create_time
	   from photo
	   left join users
	   on photo.open_id=users.open_id
	 </select>
	
	<insert id="insertPhtoByOpenId" parameterType="com.voucher.manage.model.Photo">
	   insert into photo 
       <trim prefix="(" suffix=")" suffixOverrides=",">
       campus_id,
		open_id, 
		image_url,
		create_time,
	  </trim>
	  <trim prefix="values (" suffix=")" suffixOverrides=",">
	    #{campusId,jdbcType=INTEGER},
		#{openId,jdbcType=VARCHAR}, 
	    #{imageUrl,jdbcType=VARCHAR}, 
		#{createTime,jdbcType=TIMESTAMP},
	  </trim>
	</insert>

	 
</mapper>
	
	